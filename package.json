{
	"name": "vscode-inferred-typesafe-value-generator",
	"displayName": "Inferred TypeSafe Value Generator",
	"version": "0.0.1",
	"description": "Automatically generates values inferred from type definitions.",
	"categories": [
		"Other"
	],
	"keywords": [
		"typescript",
		"javascript",
		"value generate",
		"estimate value",
		"infer value",
		"inferred value",
		"autocomplete"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/Himenon/vscode-inferred-typesafe-value-generator.git"
	},
	"license": "MIT",
	"publisher": "Himenon",
	"main": "./dist/extension.js",
	"scripts": {
		"build": "yarn run compile",
		"compile": "webpack",
		"format": "prettier \"**/*.{js,jsx,ts,tsx,json,yml,yaml,md,html}\" --write",
		"jest": "jest -c jest.config.js",
		"lint": "eslint src --ext ts",
		"package": "webpack --mode production --devtool hidden-source-map",
		"pretest:ignore": "yarn run test-compile && yarn run lint",
		"test": "yarn jest",
		"test-compile": "tsc -p tsconfig.json",
		"test-watch": "tsc -watch -p tsconfig.json",
		"test:run": "node ./out/src/test/runTest.js",
		"vscode:prepublish": "yarn run package",
		"watch": "webpack --watch"
	},
	"contributes": {
		"commands": [
			{
				"command": "infer.value.generate",
				"title": "Infer: Generate Value"
			}
		],
		"keybindings": [
			{
				"command": "infer.value.generate",
				"key": "cmd+shift+i"
			}
		],
		"menus": {
			"editor/context": [
				{
					"when": "resourceLangId =~ /^typescript|^typescriptreact$/",
					"command": "infer.value.generate",
					"group": "infer"
				}
			]
		}
	},
	"activationEvents": [
		"onCommand:infer.value.generate"
	],
	"simple-git-hooks": {
		"pre-commit": "npx lint-staged"
	},
	"lint-staged": {
		"package.json": [
			"sort-package-json"
		],
		"*.{ts,tsx,js,jsx,json,yml,yaml,html,md}": [
			"prettier --write"
		]
	},
	"dependencies": {
		"dot-prop": "6.0.1",
		"json-schema": "^0.4.0",
		"ts-clone-node": "^0.3.30",
		"ts-json-schema-generator": "^0.98.0",
		"typescript": "~4.5.0",
		"typescript-json-schema": "^0.53.0"
	},
	"devDependencies": {
		"@swc/core": "^1.2.141",
		"@swc/jest": "^0.2.17",
		"@types/glob": "^7.2.0",
		"@types/jest": "^27.4.0",
		"@types/node": "16.x",
		"@types/vscode": "^1.64.0",
		"@typescript-eslint/eslint-plugin": "^5.12.0",
		"@typescript-eslint/parser": "^5.12.0",
		"eslint": "^8.9.0",
		"glob": "^7.1.7",
		"jest": "^27.5.1",
		"lint-staged": "^12.3.4",
		"prettier": "^2.5.1",
		"simple-git-hooks": "^2.7.0",
		"sort-package-json": "^1.54.0",
		"source-map-support": "^0.5.21",
		"swc-loader": "^0.1.15",
		"ts-loader": "^9.2.2",
		"vsce": "^1.96.1",
		"vscode-test": "^1.5.2",
		"webpack": "^5.38.1",
		"webpack-cli": "^4.7.0"
	},
	"engines": {
		"vscode": "^1.64.0"
	}
}
